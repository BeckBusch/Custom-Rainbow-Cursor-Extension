{
  "manifest_version": 2,

  "name": "cursor change",
  "description": "This extension changes your cursor to any image that you want",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Cursor Change"
  },
  
 "web_accessible_resources": [
   "/cursor.png"
  ],
 
"content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["cursor.css"]
    }
  ]
}
